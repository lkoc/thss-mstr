	doi = {10.1007/s00220-011-1404-9},
	abstract = {We are concerned with the inviscid limit of the Navier-Stokes equations to the Euler equations in \$\$\{{\textbackslash}mathbb \{R\}{\textasciicircum}3\}\$\$. We first observe that a pathwise Kolmogorov hypothesis implies the uniform boundedness of the αth-order fractional derivatives of the velocity for some α {\textgreater} 0 in the space variables in L2, which is independent of the viscosity μ {\textgreater} 0. Then it is shown that this key observation yields the L2-equicontinuity in the time variable and the uniform bound in Lq, for some q {\textgreater} 2, of the velocity independent of μ {\textgreater} 0. These results lead to the strong convergence of solutions of the Navier-Stokes equations to a solution of the Euler equations in \$\$\{{\textbackslash}mathbb \{R\}{\textasciicircum}3\}\$\$. We also consider passive scalars coupled to the incompressible Navier-Stokes equations and, in this case, find the weak-star convergence for the passive scalars with a limit in the form of a Young measure (pdf depending on space and time). Not only do we offer a framework for mathematical existence theories, but also we offer a framework for the interpretation of numerical solutions through the identification of a function space in which convergence should take place, with the bounds that are independent of μ {\textgreater} 0, that is in the high Reynolds number limit.},
	pages = {267--283},
	number = {1},
	journaltitle = {Communications in Mathematical Physics},
	shortjournal = {Commun. Math. Phys.},
	author = {Chen, Gui-Qiang and Glimm, James},
	urldate = {2020-02-26},
