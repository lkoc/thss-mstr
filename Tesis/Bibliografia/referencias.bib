	volume = {226},
	issn = {03062619},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S030626191830802X},
	doi = {10.1016/j.apenergy.2018.05.085},
	abstract = {Choosing an appropriate wake loss model ({WLM}) is a critical task in predicting power production of a wind farm and performing a wind farm layout optimization. Due to their eﬃcient computational performance, analytical {WLMs}, also called kinematic models, are the most likely candidates for such applications. This paper examines the performance of six well-known analytical {WLMs}, i.e., Jensen, Larsen, Frandsen, Bastankah and Porté-Agel ({BPA}), Xie and Archer ({XA}), and Geometric Model ({GM}), by comparing their absolute error, bias, correlation coeﬃcient, and ability to predict power production within one standard deviation of the mean observed values at three major commercial wind farms: Lillgrund (oﬀshore, in Sweden), Anholt (oﬀshore, in Denmark) and Nørrekær (inland, in Denmark). The three wind farms are chosen to cover many aspects of wind farms, such as oﬀshore and inland conditions, regular and irregular layouts, and closely- to widely-spaced turbines. The conclusions of this review and the recommendations that are put forward provide practical guidelines for using analytical {WLMs} eﬀectively in future wind energy applications. Overall, the Jensen and {XA} models stand out for their consistently strong performance and for rarely (Jensen) or never ({XA}) ranking last for all wind directions at all farms and are therefore the recommended models in general.},
	pages = {1187--1207},
	journaltitle = {Applied Energy},
	shortjournal = {Applied Energy},
	author = {Archer, Cristina L. and Vasel-Be-Hagh, Ahmadreza and Yan, Chi and Wu, Sicheng and Pan, Yang and Brodie, Joseph F. and Maguire, A. Eoghan},
	urldate = {2020-01-27},
	date = {2018-09},
	langid = {english},
	file = {Archer et al. - 2018 - Review and evaluation of wake loss models for wind.pdf:/home/lkoc/Zotero/storage/VSKZGIDE/Archer et al. - 2018 - Review and evaluation of wake loss models for wind.pdf:application/pdf}
}

