@article{geller_benchmark_2006,
	title = {Benchmark computations based on lattice-Boltzmann, finite element and finite volume methods for laminar flows},
	volume = {35},
	issn = {00457930},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0045793005001519},
	doi = {10.1016/j.compfluid.2005.08.009},
	abstract = {The goal of this article is to contribute to the discussion of the eﬃciency of lattice-Boltzmann ({LB}) methods as {CFD} solvers. After a short review of the basic model and extensions, we compare the accuracy and computational eﬃciency of two research simulation codes based on the {LB} and the ﬁnite-element method ({FEM}) for two-dimensional incompressible laminar ﬂow problems with complex geometries. We also study the inﬂuence of the Mach number on the solution, since {LB} methods are weakly compressible by nature, by comparing compressible and incompressible results obtained from the {LB} code and the commercial code {CFX}. Our results indicate, that for the quantities studied (lift, drag, pressure drop) our {LB} prototype is competitive for incompressible transient problems, but asymptotically slower for steady-state Stokes ﬂow because the asymptotic algorithmic complexity of the classical {LB}-method is not optimal compared to the multigrid solvers incorporated in the {FEM} and {CFX} code. For the weakly compressible case, the {LB} approach has a signiﬁcant wall clock time advantage as compared to {CFX}. In addition, we demonstrate that the inﬂuence of the ﬁnite Mach number in {LB} simulations of incompressible ﬂow is easily underestimated.},
	pages = {888--897},
	number = {8},
	journaltitle = {Computers \& Fluids},
	shortjournal = {Computers \& Fluids},
	author = {Geller, Sebastian and Krafczyk, Manfred and Tölke, Jonas and Turek, Stefan and Hron, Jaroslav},
	urldate = {2020-01-27},
