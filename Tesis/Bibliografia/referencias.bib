	file = {Rullaud et al. - 2018 - Actuator-Line Model in a Lattice Boltzmann Framewo.pdf:/home/lkoc/Zotero/storage/V3BU9EIC/Rullaud et al. - 2018 - Actuator-Line Model in a Lattice Boltzmann Framewo.pdf:application/pdf}
}

