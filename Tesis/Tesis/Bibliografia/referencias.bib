	author = {Zhou, Lu},
	month = may,
	year = {2017},
	keywords = {Turbulence, Affouillement, Affouillement (génie hydraulique), Déformation du maillage, Ecoulement de surface libre, Free-surface flow, Local scour, Mécanique des fluides, Mesh deformation, Sediment transport, Transport de sédiments},
	file = {Snapshot:C\:\\Users\\lkoc\\Zotero\\storage\\2WMXH3GX\\2017LYSEC015.html:text/html;Zhou_2017_Numerical modelling of scour in steady flows.pdf:C\:\\Users\\lkoc\\Zotero\\storage\\SKCLQZKL\\Zhou_2017_Numerical modelling of scour in steady flows.pdf:application/pdf}
