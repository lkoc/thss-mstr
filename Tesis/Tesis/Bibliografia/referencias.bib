	pages = {345--355},
	file = {Cai et al. - 2014 - Sustainability Analysis and Resource Management fo.pdf:C\:\\Users\\lkoc\\Zotero\\storage\\KZS5XG6P\\Cai et al. - 2014 - Sustainability Analysis and Resource Management fo.pdf:application/pdf}
